module SBOM
  module CycloneDX
    module Record
      class Version < Base
        def initialize: (?version: String?, ?range: String?, ?status: String?) -> void

        def version: () -> String?
        def version=: (String?) -> String?
        def version?: () -> bool
        def version_valid?: () -> bool

        def range: () -> String?
        def range=: (String?) -> String?
        def range?: () -> bool
        def range_valid?: () -> bool

        def status: () -> String?
        def status=: (String?) -> String?
        def status?: () -> bool
        def status_valid?: () -> bool
      end
    end
  end
end
