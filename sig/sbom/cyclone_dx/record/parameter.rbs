module SBOM
  module CycloneDX
    module Record
      class Parameter < Base
        def initialize: (?name: String?, ?value: String?, ?data_type: String?) -> void

        def name: () -> String?
        def name=: (String?) -> String?
        def name?: () -> bool
        def name_valid?: () -> bool

        def value: () -> String?
